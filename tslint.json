{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "imports-order": true,
        "interface-name": [true, "never-prefix"],
        "max-classes-per-file": false,
        "max-line-length": {
            "options": [120]
        },
        "no-console": false,
        "no-empty": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "quotemark": [true, "single"],
        "variable-name": false,
    },
    "rulesDirectory": ["lint"]
}